<?xml version="1.0" encoding="utf-8"?>
<Override>
  <Item identifier="divingmask">
    <inherit identifier="divingmask">
      <doinherit />
      <replace sel="./InventoryIcon/@texture">%ModDir%/Items/Jobgear/headgears_male.png</replace>
      <replace sel="./InventoryIcon/@sourcerect">226,327,77,85</replace>
      <replace sel="./Sprite/@texture">%ModDir%/Items/Jobgear/headgears_male.png</replace>
      <replace sel="./Sprite/@sourcerect">226,327,77,85</replace>
      <replace sel="./Wearable[@limbtype='Head']/sprite/@texture">%ModDir%/Items/Jobgear/headgears_[GENDER].png</replace>
      <replace sel="./Wearable[@limbtype='Head']/sprite/@sourcerect">226,327,77,85</replace>
      <replace sel="./Wearable[@limbtype='Head']/sprite/@origin">0.5,0.5</replace>
      <add sel="./Wearable[@limbtype='Head']/sprite/@scale">0.75</add>
    </inherit>
  </Item>
